<script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
<script>
    document.querySelectorAll('.btn').forEach(function(button) {
      button.addEventListener('click', function() {
        var category = button.getAttribute('data-category');
        
        document.querySelectorAll('.tab-pane').forEach(function(tabPane) {
          tabPane.classList.remove('show', 'active');
        });
        document.querySelector('.tab-pane[data-category="' + category + '"]').classList.add('show', 'active');
      });
    });

    $(document).ready(function() {
      $('input[name="selected_stock_ids[]"]').each(function() {
        var stockId = $(this).val();
        var isSelected = localStorage.getItem('selected_stock_' + stockId);
        $(this).prop('checked', isSelected === 'true');
      });

    $('input[name="selected_stock_ids[]"]').on('change', function() {
      var stockId = $(this).val();
      var csrfToken = $('meta[name=csrf-token]').attr('content');
      var isChecked = $(this).is(':checked');

      var ajaxConfig = {
        url: isChecked ? '/api/v1/favorites' : '/api/v1/favorites/' + stockId,
        method: isChecked ? 'POST' : 'DELETE',
        headers: { 'X-CSRF-Token': csrfToken },
        data: isChecked ? { stock_id: stockId, authenticity_token: csrfToken } : {},
        success: function(response) {
          console.log('Favorite ' + (isChecked ? 'created' : 'deleted') + ' successfully for stock_id: ' + stockId);
        },
        error: function(error) {
          console.error('Error ' + (isChecked ? 'creating' : 'deleting') + ' favorite for stock_id ' + stockId + ':', error);
        }
      };
      $.ajax(ajaxConfig);
      localStorage.setItem('selected_stock_' + stockId, isChecked.toString());
    });
  });
</script>

